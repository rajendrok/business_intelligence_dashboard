{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": true,
    "checkJs": false,
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "outDir": "dist",
    "noEmit": false
  },
  "include": [
    "Homepage/**/*.ts",
    "Homepage/**/*.tsx",
    "Homepage/**/*.jsx"
  ],
  "exclude": [
    "node_modules",
    "Homepage/HomePageStyles.js",
    "Homepage/SidebarStyle.js",
    "dist"
  ],
  "extends": "expo/tsconfig.base"
}
